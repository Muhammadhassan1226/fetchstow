{
  "name": "fetchstow",
  "version": "1.0.5",
  "main": "dist/src/index.js",
  "type": "module",
  "bin": {
    "fetchstow": "dist/src/cli/index.js"
  },
  "description": "A CLI tool to fetch data from an API and store it in a JSON file",
  "scripts": {
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "build": "tsc",
    "start": "node --loader ts-node/esm --experimental-specifier-resolution=node src/cli/index.ts"
  },
  "keywords": [
    "cli",
    "fetch",
    "api",
    "json"
  ],
  "author": "Muhammad Hassan Saeed",
  "license": "ISC",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/**/*",
    "src/**/*",
    "index.ts",
    "README.md"
  ],
  "dependencies": {
    "@types/node": "^22.7.5",
    "axios": "^1.7.7",
    "commander": "^12.1.0",
    "fs-extra": "^11.2.0",
    "node-fetch": "^3.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "winston": "^3.15.0"
  },
  "devDependencies": {
    "@babel/core": "7.25.8",
    "@babel/preset-env": "7.25.8",
    "@babel/preset-typescript": "7.25.7",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.13",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5"
  }
}
